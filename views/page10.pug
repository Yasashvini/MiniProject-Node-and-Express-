</head>
<body>
<br>
<input type="text" id="myInput" onkeyup="myFunction()" placeholder="Search for Faculties" title="Type in a name">

<div class="row" id="f">
    each val,index in faculty_details
        if(index%2==0)
            <div class="column" style="background-color:#e6f3ff;" id=index>
                h2="Name:"+val.name
                p="Phone:"+val.phn
                p="Email:"+val.email
            </div>
        else
            <div class="column" style="background-color:#cce6ff;" id=index>
                h2="Name:"+val.name
                p="Phone:"+val.phn
                p="Email:"+val.email
            </div>
</div>


script
    include script1.js
